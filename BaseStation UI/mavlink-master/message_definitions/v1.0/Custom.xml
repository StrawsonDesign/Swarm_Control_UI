<?xml version='1.0'?>
<!-- MESSAGE IDs 150 - 240: Space for custom messages in individual projectname_messages.xml files -->
<mavlink>
	<include></include>
	<messages>
		<message id="150" name="Joystick_IO">
			<description> Floating point values from Joystick I/O pins (which would be converted to velocity set points using mixing on board) </description>
			<field type="uint16_t" name="Joystick_chan1_raw"> Joystick Channel 1 </field>
			<field type="uint16_t" name="Joystick_chan2_raw"> Joystick Channel 2 </field>
			<field type="uint16_t" name="Joystick_chan3_raw"> Joystick Channel 3 </field>
			<field type="uint16_t" name="Joystick_chan4_raw"> Joystick Channel 4 </field>
			<field type="uint16_t" name="Joystick_chan5_raw"> Joystick Channel 5 </field>
			<field type="uint16_t" name="Joystick_chan6_raw"> Joystick Channel 6 </field>
			<field type="uint16_t" name="Joystick_chan7_raw"> Joystick Channel 7 </field>
			<field type="uint16_t" name="Joystick_chan8_raw"> Joystick Channel 8 </field>
		</message>
		<message id="151" name="ctl_mode_packet">
			<description> Twelve different modes for the hexarotor. </description>
			<field type="uint16_t"		name="Slam_Mode_chan1_raw"> Slam Mapping Mode </field>
			<field type="uint16_t"		name="Vicon_Mode_chan2_raw"> Vicon Position Mapping Mode </field>
