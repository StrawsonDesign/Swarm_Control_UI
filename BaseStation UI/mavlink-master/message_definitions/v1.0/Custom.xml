			<field type="uint16_t"		name="Global_Mode_chan6_raw"> Global Pilot Reference Mode </field>
			<field type="uint16_t"		name="FPV_Mode_chan7_raw"> First Person View Mode </field>
			<field type="uint16_t"		name="PPV_Mode_chan8_raw"> PPV Pilot Reference Mode </field>
			<field type="uint16_t"		name="User_Mode_chan9_raw"> User Control Mode </field>
			<field type="uint16_t"		name="Autoc_Mode_chan10_raw"> Auto Land Control Mode </field>
			<field type="uint16_t"		name="Return_Mode_chan11_raw"> Return Home Contrl Mode </field>
			<field type="uint16_t"		name="Hover_Mode_chan12_raw"> Hover Control Mode </field>
